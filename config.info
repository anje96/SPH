; h5 file containing the initial particle distribution
initFile rings.h5

; output directory for particle distribution h5 files
outDir output

; fixed time step
timeStep .01

; end of the simulation
timeEnd 100

; output particle distribution each h5DumpInterval steps
h5DumpInterval 50

; Sound speed
speedOfSound 1.

; smoothing length
smoothingLength 0.3

; density in relaxed state, should be zero for gas
restDensity 1.0

; maximum number of nearest neighbors
maxNN 300

; shear modulus, ignored when SOLIDS not set in parameter.h
shearModulus 0.22